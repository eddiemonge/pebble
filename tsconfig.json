{
  "compilerOptions": {
    "allowJs": true,
    "jsx": "react",
    "noEmit": true,
    "noResolve": true,
    "isolatedModules": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true
  }
}
